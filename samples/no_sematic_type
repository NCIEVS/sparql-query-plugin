prefix ncit: <http://ncicb.nci.nih.gov/xml/owl/EVS/Thesaurus.owl#>
prefix owl:  <http://www.w3.org/2002/07/owl#>
prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>
SELECT distinct ?s ?l
where { 
	?s ?p ?o .
	?s a owl:Class .
	optional {?s rdfs:label ?l .}
	minus { ?s ncit:Semantic_Type ?x. }
	}
LIMIT 15